                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="15dp"
                    android:text="@string/sample_17_coworker"
                    />
                <CheckBox
                    android:id="@+id/widget_18_friends"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="15dp"
                    android:text="@string/sample_18_friends"
                    />
                <CheckBox
                    android:id="@+id/widget_19_girl_boyfriend"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="15dp"
                    android:text="@string/sample_19_girl_boyfriend"
                    />
                <CheckBox
                    android:id="@+id/widget_20_mother"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="15dp"
                    android:text="@string/sample_20_mother"
                    />
                <CheckBox
                    android:id="@+id/widget_21_father"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="15dp"
                    android:text="@string/sample_21_father"
                    />
                <CheckBox
                    android:id="@+id/widget_22_teacher"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="15dp"
                    android:text="@string/sample_22_teacher"
                    />
                <CheckBox
                    android:id="@+id/widget_23_classmate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="15dp"
                    android:text="@string/sample_23_classmate"
                    />
                <CheckBox
                    android:id="@+id/widget_24_other"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="15dp"
                    android:text="@string/sample_24_other"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <CheckBox
