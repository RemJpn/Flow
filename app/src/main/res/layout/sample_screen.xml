                android:layout_height="wrap_content"
                android:text="@string/sample_11_alone"/>
            <RadioGroup
                android:id="@+id/alone_radio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                >
                <RadioButton
                    android:id="@+id/sample_12_alone_yes"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/sample_12_alone_yes"
                    />
                <RadioButton
                    android:id="@+id/sample_13_alone_no"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/sample_13_alone_no"
                    />
            </RadioGroup>

            <LinearLayout
